APP_NAME="Budget Tours"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost:8000

LOG_CHANNEL=stack
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=budget_tours
DB_USERNAME=root
DB_PASSWORD=

BROADCAST_DRIVER=log
CACHE_DRIVER=file
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=smtp
MAIL_HOST=smtp.mailtrap.io
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=tls
MAIL_FROM_ADDRESS=info@budgettours.com
MAIL_FROM_NAME="${APP_NAME}"

# File Upload Settings
FILESYSTEM_DISK=public
MAX_UPLOAD_SIZE=10240

# Payment Gateway Settings
STRIPE_KEY=
STRIPE_SECRET=
STRIPE_WEBHOOK_SECRET=

# Google Maps API
GOOGLE_MAPS_API_KEY=

# Social Media Login
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
FACEBOOK_CLIENT_ID=
FACEBOOK_CLIENT_SECRET=

# Cache Settings
CACHE_PREFIX=budget_tours_
CACHE_DEFAULT_TTL=3600

# Security Settings
SESSION_SECURE_COOKIE=false
SANCTUM_STATEFUL_DOMAINS=localhost:8000

# Application Settings
DEFAULT_CURRENCY=USD
DEFAULT_TIMEZONE=UTC
BOOKING_CONFIRMATION_EXPIRY=24

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
